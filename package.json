{
  "name": "mockend",
  "version": "0.0.1",
  "description": "An app for mocking backends",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "ts-node-dev src/server.ts",
    "deploy": "git push && git push heroku master",
    "pretest": "NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "test": "jest --runInBand --coverage",
    "sql": "node_modules/.bin/sequelize",
    "eslint": "node_modules/.bin/eslint",
    "migrate-db": "node_modules/.bin/sequelize db:migrate",
    "prod-logs": "heroku logs --tail",
    "g": "./scripts/generations.sh",
    "cov-report": "open ./coverage/lcov-report/index.html",
    "db-init": "yarn sql db:create && NODE_ENV=test yarn sql db:create",
    "service-init": "yarn submodule-init && yarn db-init"
  },
  "keywords": [
    "mock",
    "api",
    "backend"
  ],
  "author": "Kito Clark",
  "license": "ISC",
  "dependencies": {
    "adjective-adjective-animal": "^1.3.3",
    "array-sort": "^1.0.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "fast-deep-equal": "^3.1.1",
    "lodash": "^4.17.15",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local-sequelize": "^0.9.0",
    "pg": "^7.14.0",
    "pluralize": "^8.0.0",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@types/array-sort": "^1.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.29",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.8",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.21",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
